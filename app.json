{
  "expo": {
    "name": "HarmoniX",
    "slug": "meu-primeiro-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "meuprimeiroapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo-inicio.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "navigationBar": {
      "backgroundColor": "#1a1a1a",
      "barStyle": "light-content",
      "visible": "true"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "backgroundColorDark": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.br12zin.harmonix"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "83df7a5b-ce0a-447f-a433-d6dafbcedaac"
      }
    },
    "owner": "br12zin"
  }
}
